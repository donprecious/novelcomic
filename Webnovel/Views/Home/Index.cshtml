@using Webnovel.Repository
@using CloudinaryDotNet
@using Webnovel.Helpers
@using Webnovel.Services

@{
    ViewData["Title"] = "Home Page";
}
@inject INovel _novel
@inject IComic _comic 
@inject  IAnimation _animation
@inject ICategory _category
@inject IRate _rate
@{
    var novels = await _novel.GetAllNovels(false,true);
    var comics = await _comic.GetAllComics(false,true);
    var categories = await _category.List();
    var animations = await _animation.GetAllAnimations();
    var tags = await _novel.Tags();
    var cloudinary = new Cloudinary(new Account()
    {
        ApiKey = CloudinaryUpload.apiKey,
        ApiSecret = CloudinaryUpload.apiSecret,
        Cloud = CloudinaryUpload.cloud,

    });
}

<div class="slider movie-items">
    <div class="container">
        <div class="row">
            @*<div class="social-link">
                    <p>Follow us: </p>
                    <a href="#"><i class="ion-social-facebook"></i></a>
                    <a href="#"><i class="ion-social-twitter"></i></a>
                    <a href="#"><i class="ion-social-googleplus"></i></a>
                    <a href="#"><i class="ion-social-youtube"></i></a>
                </div>*@
            <div class="slick-multiItemSlider">
                @foreach(var i in categories)
                {
                         <div class="movie-item">
                    <div class="mv-img">
                        <a href="#"><img data-src="../home/images/uploads/slider4.jpg" alt="@i.Name" class="lazyload" width="200" height="437"></a>
                    </div>
                    <div class="title-in">
                        <div class="cate">
                            <span class="blue"><a href="#">@i.Name</a></span>
                        </div>
                        <h6><a href="#">@i.Description</a></h6>
                        
                    </div>
                </div>

                }
           
        
            </div>
        </div>
    </div>
</div>


<div class="movie-items">
    <div class="container">
        <div class="row ipad-width">
            <div class="col-md-8">
                <div class="title-hd">
                    <h2>Novels</h2>
                    <a href="@Url.Action("Novels")" class="viewall">View all <i class="ion-ios-arrow-right"></i></a>
                </div>
                <div class="tabs">
                    <ul class="tab-links">
                        <li class="active"><a href="#tab1">#Popular</a></li>
                        <li><a href="#tab2"> #This Week</a></li>
                        <li><a href="#tab3">  #This Month  </a></li>
                        <li><a href="#tab4"> #Most Rated</a></li>
                    </ul>
                    <div class="tab-content">
                        <div id="tab1" class="tab active">
                            <div class="row">
                                <div class="slick-multiItem">
                                    @foreach (var i in novels)
                                    {
                                        var img = i.CoverPageImageUrl;
                                        if (img != null)
                                        {
                                            img = cloudinary.Api.UrlImgFetch.
                                                Transform(new Transformation()
                                                    .Height(244).Width(152).Crop("fill")).BuildUrl(i.CoverPageImageUrl);

                                        }
                                        else
                                        {
                                            img = cloudinary.Api.UrlImgFetch.
                                                Transform(new Transformation()
                                                    .Height(234).Width(152).Crop("fill")).BuildUrl("https://res.cloudinary.com/votel/image/upload/v1562332529/NO_COVER_PAGE_1.png");

                                        }

                                        <div class="slide-it slick-side">
                                            <div class="movie-item">
                                                @*<a data-toggle="popover" data-placement="right" title="@i.Name" data-title="@i.Name" data-content="@i.Title">*@
                                                    <div class="mv-img">
                                                        <img data-src="@img" alt="@i.Name" title="@i.Title" class="lazyload" width="185" height="284">
                                                    </div>
                                                    <div class="hvr-inner">
                                                        <a href="@Url.Action("Novel", new {id = @i.Id})"> Read <i class="ion-android-arrow-dropright"></i> </a>
                                                    </div>
                                                    <div class="title-in">
                                                        <h6><a href="@Url.Action("Novel", new {id = @i.Id})" title="@i.Name" aria-describedby="@i.Title">@StringProcessor.SubString(@i.Name, 0, 20) </a></h6>
                                                        <p><i class="ion-android-star"></i><span>@await _rate.GetNovelRateAverage(i.Id)</span> /5.0</p>
                                                    </div>
                                                @*</a>*@

                                            </div>
                                        </div>


                                    }

                                </div>
                            </div>
                        </div>
                
                    </div>
